# -*- coding: utf-8 -*-

o = input()
mat = [0] * 12
for i in range(12):
    mat[i] = [0] * 12

for i in range(12):
    for j in range(12):
        mat[i][j] = float(input())
       
soma = 0

for i in range(1, 6):
    for j in range(11, 11 - i, -1):
        soma += mat[i][j]
for i in range(6, 11):
    for j in range(i + 1, 12):
        soma += mat[i][j]

if o == 'S':
    print('%.1f' % soma)
else:
    print('%.1f' % soma / 30)